msgid "Add Institutions"
msgstr "Ajouter institutions"

#: importer/templates/simple_import_index.html
msgid "Add Teams"
msgstr "Ajouter équipes"

#: importer/templates/simple_import_index.html
msgid "Add Adjudicators"
msgstr "Ajouter juges"

